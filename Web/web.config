<configuration>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
  </system.web>

	<system.webServer>
		<handlers>
			<add name="CompassSASS" verb="*" path="*.sass" type="Dado.Compass.SingleFileHandler, Dado.Compass" resourceType="File" preCondition="" />
			<add name="CompassSCSS" verb="*" path="*.scss" type="Dado.Compass.SingleFileHandler, Dado.Compass" resourceType="File" preCondition="" />
		</handlers>
	</system.webServer>
</configuration>
